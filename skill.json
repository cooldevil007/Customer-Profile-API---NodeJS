{
  "manifest": {
    "publishingInformation": {
      "locales": {
        "en-US": {
          "summary": "Sample Short Description",
          "examplePhrases": [
            "Alexa open customer profiler",
            "Alexa tell customer profiler for mobile number",
            "Alexa ask customer profiler for what is my email"
          ],
          "name": "customer-profile",
          "description": "A starter template for Alexa skill that uses the Customer Profile API",
          "smallIconUri": "https://s3.amazonaws.com/alexa-audioimages/cartoon-animation.png",
          "largeIconUri": "https://s3.amazonaws.com/alexa-audioimages/cartoon-animation-large.png"
        }
      },
      "isAvailableWorldwide": true,
      "testingInstructions": "Sample Testing Instructions.",
      "category": "EDUCATION_AND_REFERENCE",
      "distributionCountries": []
    },
    "apis": {
      "custom": {
        "endpoint": {
          "sourceDir": "lambda/custom"
        }
      }
    },
    "manifestVersion": "1.0",
    "privacyAndCompliance": {
      "allowsPurchases": false,
      "locales": {
        "en-US": {
          "termsOfUseUrl": "http://example.com/terms",
          "privacyPolicyUrl": "http://example.com/privacy"
        }
      },
      "isExportCompliant": true,
      "containsAds": false,
      "isChildDirected": false,
      "usesPersonalInfo": false
    },
    "permissions": [
      {
        "name": "alexa::profile:email:read"
      },
      {
        "name": "alexa::profile:name:read"
      },
      {
        "name": "alexa::profile:mobile_number:read"
      }
    ]
  }
}
